/*
  HealthNet CSS Theme
  ==============================================================================

  Body
  Text Styles
  Position Styles
  Sizing Styles
  Decoration Styles
  Form Styles

*/

/* Fonts */
@import url(https://fonts.googleapis.com/css?family=Ubuntu:400,700);
@import url(https://fonts.googleapis.com/css?family=Open+Sans:400,700);

/*
  ================================================================== Animations
*/
@keyframes rotate-45 {
    from { transform: rotate(0deg); }
    to { transform: rotate(45deg); }
}
@-webkit-keyframes rotate-45 {
    from { transform: rotate(0deg); }
    to { transform: rotate(45deg); }
}
@-moz-keyframes rotate-45 {
    from { transform: rotate(0deg); }
    to { transform: rotate(45deg); }
}
@-o-keyframes rotate-45 {
    from { transform: rotate(0deg); }
    to { transform: rotate(45deg); }
}
.rotate-45 {
    animation:
        rotate-45 .4s forwards;
}

@keyframes rotate-minus-45 {
    from { transform: rotate(0deg); }
    to { transform: rotate(-45deg); }
}
@-webkit-keyframes rotate-minus-45 {
    from { transform: rotate(0deg); }
    to { transform: rotate(-45deg); }
}
@-moz-keyframes rotate-minus-45 {
    from { transform: rotate(0deg); }
    to { transform: rotate(-45deg); }
}
@-o-keyframes rotate-minus-45 {
    from { transform: rotate(0deg); }
    to { transform: rotate(-45deg); }
}
.rotate-minus-45 {
    animation:
        rotate-minus-45 .4s forwards;
}

@keyframes rotate-0 {
    to { transform: rotate(0deg); }
}
@-webkit-keyframes rotate-0 {
    to { transform: rotate(0deg); }
}
@-moz-keyframes rotate-0 {
    to { transform: rotate(0deg); }
}
@-o-keyframes rotate-0 {
    to { transform: rotate(0deg); }
}
.rotate-0 {
    animation:
        rotate-0 .4s forwards;
}

@keyframes fade-grow-in {
    from { opacity: 0; transform: scale(0); }
    to { opacity: 1; transform: scale(1); }
}
@-webkit-keyframes fade-grow-in {
    from { opacity: 0; transform: scale(0); }
    to { opacity: 1; transform: scale(1); }
}
@-moz-keyframes fade-grow-in {
    from { opacity: 0; transform: scale(0); }
    to { opacity: 1; transform: scale(1); }
}
@-o-keyframes fade-grow-in {
    from { opacity: 0; transform: scale(0); }
    to { opacity: 1; transform: scale(1); }
}
.fade-grow-in {
    animation:
        fade-grow-in .2s .2s cubic-bezier(0,.8,.32,1.07) forwards;
}

@keyframes fade-grow-out {
    from { opacity: 1; transform: scale(1); }
    to { opacity: 0; transform: scale(0); }
}
@-webkit-keyframes fade-grow-out {
    from { opacity: 1; transform: scale(1); }
    to { opacity: 0; transform: scale(0); }
}
@-moz-keyframes fade-grow-out {
    from { opacity: 1; transform: scale(1); }
    to { opacity: 0; transform: scale(0); }
}
@-o-keyframes fade-grow-out {
    from { opacity: 1; transform: scale(1); }
    to { opacity: 0; transform: scale(0); }
}
.fade-grow-out {
    animation: fade-grow-out .6s cubic-bezier(.55,-0.04,.91,.94) forwards;
}

@keyframes in-from-bottom {
  from { margin-top: 2000px; }
}
@-webkit-keyframes in-from-bottom {
  from { margin-top: 2000px; }
}
@-moz-keyframes in-from-bottom {
  from { margin-top: 2000px; }
}
@-o-keyframes in-from-bottom {
  from { margin-top: 2000px; }
}
.animate-from-bottom {
  animation-name: in-from-bottom;
  animation-duration: .7s;
  -webkit-animation-name: in-from-bottom;
  -webkit-animation-duration: .7s;
  -moz-animation-name: in-from-bottom;
  -moz-animation-duration: .7s;
  -o-animation-name: in-from-bottom;
  -o-animation-duration: .7s;
}

@keyframes in-from-bottom-long {
  from { margin-top: 2000px; }
}
@-webkit-keyframes in-from-bottom-long {
  from { margin-top: 2000px; }
}
@-moz-keyframes in-from-bottom-long {
  from { margin-top: 2000px; }
}
@-o-keyframes in-from-bottom-long {
  from { margin-top: 2000px; }
}
.animate-from-bottom-long {
  animation-name: in-from-bottom-long;
  animation-duration: 1.2s;
  -webkit-animation-name: in-from-bottom-long;
  -webkit-animation-duration: 1.2s;
  -moz-animation-name: in-from-bottom-long;
  -moz-animation-duration: 1.2s;
  -o-animation-name: in-from-bottom-long;
  -o-animation-duration: 1.2s;
}

@keyframes animate-fade-out {
  from { opacity: 100%; }
  to { opacity: 0%; }
}
@-webkit-keyframes animate-fade-out {
  from { opacity: 100%; }
  to { opacity: 0%; }
}
@-moz-keyframes animate-fade-out {
  from { opacity: 100%; }
  to { opacity: 0%; }
}
@-o-keyframes animate-fade-out {
  from { opacity: 100%; }
  to { opacity: 0%; }
}
.animate-fade-out {

  animation-name: animate-fade-out;
  animation-duration: 5s;
  -webkit-animation-name: animate-fade-out;
  -webkit-animation-duration: 5s;
  -moz-animation-name: animate-fade-out;
  -moz-animation-duration: 5s;
  -o-animation-name: animate-fade-out;
  -o-animation-duration: 5s;
}

/*
  ======================================================================== Body
*/
body {
  background-color: #921717;
}

.content {
  z-index: 4;
}

/*
  ================================================================= Text Styles
*/
h1 {
  font-size: 32px;
  margin-bottom: 20px;
  margin-top: 0;
  font-family: 'Ubuntu', sans-serif;
  color: #000000;
}

h2 {
  font-size: 24px;
  margin-bottom: 15px;
  font-family: 'Ubuntu', sans-serif;
  color: #000000;
}

h3 {
  font-size: 20px;
  margin-bottom: 10px;
  font-family: 'Ubuntu', sans-serif;
  color: #000000;
}

p {
  margin-bottom: 10px;
  font-size: 16px;
  font-family: 'Open Sans', sans-serif;
  line-height: 25px;
  color: #333333;
}

a {
  margin-bottom: 8px;
  font-size: 16px;
  font-family: 'Open Sans', sans-serif;
  color: #B81D1D;
}

a:hover {
  color: #921717;
}

a:visited {
  color: #000000;
}

a:focus {
  outline-color: #D22121;
  color: #D22121;
}


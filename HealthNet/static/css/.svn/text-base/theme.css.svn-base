  /*border-top-right-radius: 5px;
  border-bottom-right-radius: 5px;*/
}

.text-white {
  color: #ffffff;
}

.sep-dashed-grey {
  border-bottom: 2px dashed #cccccc;
  width: 90%;
  margin: 5px auto 10px auto;
}

.sep-solid-grey {
  border-bottom: 2px solid #cccccc;
  width: 90%;
  margin: 5px auto 10px auto;
}

.rcorner-top-left {
  border-top-left-radius: 5px;
}

.rcorner-top-right {
  border-top-right-radius: 5px;
}

.rcorner-bottom-left {
  border-bottom-left-radius: 5px;
}

.rcorner-bottom-right {
  border-bottom-right-radius: 5px;
}

.arrow-box-left {
    border: 1px solid #ff5050;
    border-radius: 3px;
    -webkit-filter: drop-shadow(0 1px 10px rgba(0,0,0,0.5));
    -moz-box-shadow: 0 1px 10px rgba(0,0,0,0.5);
    filter: drop-shadow(0 1px 10px rgba(0,0,0,0.5));
}

.arrow-box-left:after,
.arrow-box-left:before {
    bottom: 85%;
    right: 100%;
    border: solid transparent;
    content: " ";
    height: 0;
    width: 0;
    position: absolute;
    pointer-events: none;
}

.arrow-box-left:after {
    border-color: rgba(255, 255, 255, 0);
    border-right-color: #ffffff;
    border-width: 20px;
    left: 0%;
    margin-left: -39px;
}

.arrow-box-left:before {
    border-color: rgba(255, 80, 80, 0);
    border-right-color: #ff5050;
    border-width: 20px;
    left: 0%;
    margin-left: -40px;
}

/*
  ================================================================= Form Styles
*/
input {
  font-family: 'Ubuntu', sans-serif;
  background-color: #dddddd;
  font-size: 18px;
  height: 40px;
  width: 170px;
  margin: 0 auto 20px auto;
  padding: 3px;
  border-radius: 2px;
}

input:focus {
  outline-color: #DF2323;
}

textarea {
  font-family: 'Ubuntu', sans-serif;
  background-color: #dddddd;
  font-size: 18px;
  height: 40px;
  width: 170px;
  max-width: 100%;
  margin: 0 auto 20px auto;
  padding: 3px;
}

textarea:focus {
  outline-color: #D22121;
}

input[type="text"] {
  font-family: 'Ubuntu', sans-serif;
  background-color: #dddddd;
  font-size: 18px;
  height: 40px;
  width: 170px;
  margin: 0 auto 20px auto;
  padding: 3px;
}

input[type="text"]:focus {
  outline-color: #D22121;
}

input[type="password"] {
  font-family: 'Ubuntu', sans-serif;
  background-color: #dddddd;
  font-size: 18px;
  height: 40px;
  width: 170px;
  margin: 0 auto 20px auto;
  padding: 3px;
}

input[type="password"]:focus {
  outline-color: #D22121;
}

input[type="email"] {
  font-family: 'Ubuntu', sans-serif;
  background-color: #dddddd;
  font-size: 18px;
  height: 40px;
  width: 170px;
  margin: 0 auto 20px auto;
  padding: 3px;
}

input[type="email"]:focus {
  outline-color: #D22121;
}

button {
  font-family: 'Ubuntu', sans-serif;
  height: 40px;
  width: 170px;
  margin: 20px auto 20px auto;
  padding: 3px;
  font-size: 24px;
  text-align: center;
}

button:focus {
  outline-color: #D22121;
}

select {
  background-color: #dddddd;
  font-family: 'Ubuntu', sans-serif;
  height: 40px;
  width: 100%;
  margin: 0px auto 20px auto;
  padding: 3px;
  font-size: 24px;
  text-align: center;
}

select:focus {
  outline-color: #D22121;
}

input[type="submit"] {
  font-family: 'Ubuntu', sans-serif;
  height: 40px;
  width: 170px;
  margin: 20px auto 20px auto;
  padding: 3px;
  font-size: 24px;
}

input[type="submit"]:focus {
  outline-color: #D22121;
}

label {
  font-family: 'Ubuntu', sans-serif;
  font-weight: bold;
  font-size: 18px;
  color: #ffffff;
}

input[type="file"] {
  font-family: 'Ubuntu', sans-serif;
  background: none;
  font-size: 18px;
  height: 40px;
  width: 100%;
  margin: 0 auto 20px auto;
  padding: 3px;
  border-radius: 5px;
  text-align: center;
  vertical-align: middle;
}

label.file-upload-label input[type="file"] {
    position: fixed;
    top: -1000px;
}

.file-upload-label {
    border-bottom: 1px solid #AAA;
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    padding: 2px 5px;
    margin: 2px;
    font-family: 'Ubuntu', sans-serif;
    background: #f0f0f0;
    width: 100%;
    display: inline-block;
}

.file-upload-label span {
    color: #333333;
}

.file-upload-label:hover {
    background: #e3dede;
    cursor: pointer;
    cursor: hand;
}

.file-upload-label:active {
    background: ##e3dede;
}


}

.padding-10 {
  padding: 10px;
}

.side-margins-50px {
  margin-left: 50px;
  margin-right: 50px;
}

.side-margins-25px {
  margin-left: 25px;
  margin-right: 25px;
}

.side-margins-10px {
  margin-left: 10px;
  margin-right: 10px;
}

.word-break {
  word-break: break-all;
}

/*
  =============================================================== Sizing Styles
*/
.width-auto {
  width: auto;
}

.width-100 {
  width: 100%;
}

.width-75 {
  width: 75%;
}

.width-50 {
  width: 50%;
}

.width-auto {
  width: auto;
}

.scale-0 {
  transform: scale(0);
}

.scale-1 {
  transform: scale(1);
}


/*
=========================================================== Profile Edit Styles
*/
.profile-edit-button {
  position: absolute;
  right: 6px;
  top: 6px;
  width: 80px;
  height: 25px;
  padding-bottom: 2.5px;

  border-radius: 5px;
  background-color: #c9c6c6;
  border-bottom: 2px solid #908f8f;
  border-right: 1px solid #908f8f;

  /*opacity: 0;*/

  transition-property: opacity;
  transition-duration: .3s;
}

.profile-edit-button i {
  width: 26px;
  height: 25px;
  padding-top: 2.5px;
  padding-left: 5px;
  float: left;

  border-top-left-radius: 5px;
  border-bottom-left-radius: 5px;
  border-bottom: 2px solid #a81919;
  background-color: #D22121;
  color: #efefef;
  font-size: 18px;
}

.profile-edit-button p {
  width: 52px;
  height: 20px;
  margin: 0;
  padding: 0;
  float: left;

  text-align: center;
  font-size: 18px;
  font-weight: bold;
  font-family: 'Ubuntu', sans-serif;
}

#basic-info-tab:hover .profile-edit-button {
  cursor: hand;
  cursor: pointer;

  opacity: 1;

  transition-property: opacity;
  transition-duration: .3s;
}

#medical-info-tab:hover .profile-edit-button {
  cursor: hand;
  cursor: pointer;

  opacity: 1;

  transition-property: opacity;
  transition-duration: .3s;
}

#basic-info-edit, #medical-info-edit,
#add-contact-popup, #edit-contact-popup,
#view-appointment-container, #create-appointment-popup,
#new-message-popup, #view-message-popup {
  width: 90%;
}

#edit-appointment-content {
  width: 100%;
}

#edit-appointment-button:hover {
  text-decoration: none;
}

/*
========================================================== Notifications Styles
*/

#notifications-pane {
  position: absolute;
  top: 95;
  left: 90px;
  height: 300px;
  width: 400px;

  background-color: #ffffff;
  z-index: 103;
}

#notifications-pane h3 {
  margin-top: 6px;
  margin-bottom: 2px;
}

#notifications-box {
  padding: 20px;
  padding-top: 5px;
  padding-bottom: 5px;
  width: 100%;
  height: 250px;
  overflow-x: hidden;
  overflow-y: auto;
}

#notifications-pane ul {
  list-style: none;
  margin: 0;
  padding: 0;
}

#notifications-pane li {
  list-style: none;
  margin: 0;
  margin-bottom: 2px;
  padding: 10px;
  padding-left: 10px;
  background-color: #eeeded;
  border-bottom: 2px solid #D5D4D4;
  border-right: 1px solid #D5D4D4;
  border-radius: 5px;

  transition-property: background-color;
  transition-duration: .2s;
}

#notifications-pane li:hover {
  cursor: pointer;
  cursor: hand;
  background-color: #b82424;

  transition-property: background-color;
  transition-duration: .2s;
}

#notifications-pane p {
  display: inline-block;
  margin: 0;
  padding: 0;
}

#notifications-pane li:hover p {
  color: #ffffff;

  transition-property: color;
  transition-duration: .2s;
}

.notification-dot {
  display: inline-block;
  width: 12px;
  height: 12px;
  margin-right: 15px;

  background-color: #D22121;
  border-radius: 50%;

  transition-property: background-color;
  transition-duration: .2s;
}

li:hover .notification-dot {
  background-color: #ffffff;

  transition-property: background-color;
  transition-duration: .2s;
}

#notifications-pane::-webkit-scrollbar {
    width: 10px;
    background: rgba(220, 220, 220, 0);
}

#notifications-pane::-webkit-scrollbar-thumb {
    background: #b1b1b1;
    -webkit-border-radius: 1ex;
}

#notifications-pane::-webkit-scrollbar-corner {
    background: rgba(220, 220, 220, 0);
}


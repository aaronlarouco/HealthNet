/*
=============================================================== General Styles
*/

#profile-body {
    overflow: hidden;
}

.profile-content {
    z-index: 4;
    margin-left: 8%;
    margin-right: 8%;
    width: 88%;
    height: 100%;
}

/*
========================================================== Profile Pane Styles
*/

.profile-pane {
  width: 100%;
  margin-top: 30px;
  margin-left: auto;
  margin-right: auto;
}

@media screen and (min-width: 1436px) {
  #basic-info-pane {
    width: 90%;
  }
}
@media screen and (max-width: 1435px) {
  #basic-info-pane {
    width: 100%;
  }
}

/*
======================================================== Profile Picture Styles
*/

@media screen and (min-width: 1331px) {
  #profile-picture {
    width: 100%;
  }
}
@media screen and (max-width: 1330px) {
  #profile-picture {
    width: 100%;
  }
}

#profile-picture h1 {
    font-size: 28px;
    background-color: #e6e6e6;
    padding: 10px;
    border-bottom-right-radius: 5px;
    border-bottom-left-radius: 5px;
    width: 100%;
    text-align: center;
    margin: 0 auto 0 auto;
}

#profile-pic-edit {
    position: absolute;
    top: 0px;
    bottom: 0px;
    left: 0px;
    right: 0px;
}

#profile-pic-area {
    position: relative;
    max-width: 100%;
    margin: 0 auto 0 auto;
    text-align: center;
}

#profile-pic-overlay {
    position: absolute;
    top: 0px;
    bottom: 0px;
    left: 0px;
    right: 0px;
    background-color: rgba(0, 0, 0, 0.60);
}

#profile-pic-edit-overlay {
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.60);
    text-align: center;
}

#profile-pic-edit-icon {
    position: absolute;
    width: 64px;
    height: 64px;
    top: 20%;
    left: 50%;
    transform: translate(-50%);
    background-color: rgba(200, 200, 200, 0.70);
    text-align: center;
    padding: 20px;
    border-radius: 32px;
}

#profile-pic-edit-icon:hover {
    cursor: pointer;
    cursor: hand;
}

#profile-pic-edit-icon i {
    font-size: 24px;
    color: #000000;
}

#profile-pic-edit-icon p {
    position: absolute;
    text-align: center;
    font-size: 24px;
    top: 70px;
    left: 50%;
    transform: translate(-50%);
    font-weight: bold;
    color: #ffffff;
}

#profile-pic-pic {
    max-width: 100%;
    margin: 0 auto 0 auto;
    text-align: center;
}

/*
============================================================ Quick Links Styles
*/

@media screen and (max-width: 767px) {
  #profile-quick-links {
    display: none;
  }
}

#profile-quick-links {
  width: 112px;
  margin: 30px 0 0 0;
}

#profile-quick-links-inner {
  padding: 5px;
}

.profile-quick-link-button {
  height: 38px;
  display: inline-block;
  padding: 9px;
  border-radius: 5px;
  vertical-align: middle;
  margin-left: 5px;
  margin-right: 5px;

  color: #000000;
  background-color: #d5d5d5;
  border-bottom: 2px solid #bfbfbf;
  border-right: 1px solid #bfbfbf;

  transition-property: background-color, color;
  transition-duration: .4s, .4s;
}

.profile-quick-link-button:hover {
  cursor: pointer;
  cursor: hand;
  background-color: #D22121;
  color: #ffffff;

  transition-property: background-color, color;
  transition-duration: .3s, .3s;
}

.profile-quick-link-button:hover i {
  color: #ffffff;

  transition-property: color;
  transition-duration: .3s;
}

.profile-quick-link-button:hover p {
  color: #ffffff;

  transition-property: color;
  transition-duration: .3s;
}

.profile-quick-link-button i {
  display: inline-block;
  padding: 0;
  margin: 0;
  margin-right: 10px;
  font-size: 20px;
  line-height: 20px;
  color: #000000;
}

.profile-quick-link-button p {
  display: inline-block;
  margin: 0;
  padding: 0;
  font-size: 20px;
  line-height: 20px;
  font-family: 'Ubuntu', sans-serif;
}

#msg-tools {
    width: 235px;
}

#cal-tools {
  width: 248px;
}

#side-quick-links {
  width: 60px;
}

.side-quick-links-inner {
  width: 100%;
  height: 100%;
}

/*
============================================================ Basic Info Styles
*/

.profile-address-spacing {
    position: relative;
    height: 40px;
    background-color: #e6e6e6;
}

.address-field-icon {
    width: 40px;
    height: 40px;
    font-size: 18px;
    background-color: #D22121;
    color: #ffffff;
    padding: 11px;
    margin: 0;
}

.profile-field-icon {
    width: 40px;
    height: 40px;
    font-size: 18px;
    background-color: #D22121;
    color: #ffffff;
    padding: 11px;
    margin: 0;
    border-top-left-radius: 5px;
    border-bottom-left-radius: 5px;
}

.profile-info {
    font-size: 18px;
    height: 40px;
    display: inline;
    margin-left: 10px;
}

.profile-info-spacing {
    position: relative;
    height: 40px;
    background-color: #e6e6e6;
    margin-bottom: 10px;
    border-radius: 6px;
    border-top-right-radius: 5px;
    border-bottom-right-radius: 5px;
}

#basic-info-content h3 {
  color: #777777;
  font-size: 18px;
  margin: 0px;
  margin-bottom: 2px;
  padding: 0px;
}

#medical-info-content h3 {
  color: #777777;
  font-size: 18px;
  margin: 0px;
  margin-bottom: 2px;
  padding: 0px;
}

@media screen and (max-width: 767px) {
  #basic-info-content h3 {
    display: none;
  }

  #medical-info-content h3 {
    display: none;
  }
}

